{
  "name": "karma-static",
  "description": "Serve a directory of static files on the Karma server.",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .mjs .",
    "prepare": "babel '*.mjs' --out-dir .",
    "watch": "babel '*.mjs' --watch --extensions .mjs --out-dir ."
  },
  "dependencies": {
    "serve-static": "^1.13.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "jacksonrayhamilton/babel-plugin-transform-es2015-modules-commonjs#b0b9a88",
    "eslint": "^4.12.0",
    "eslint-config-will-robinson": "^1.0.0"
  }
}
